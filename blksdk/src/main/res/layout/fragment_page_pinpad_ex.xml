        android:layout_height="60dp"
        android:layout_alignTop="@+id/KBD5"
        android:layout_marginStart="13dp"
        android:layout_toEndOf="@+id/KBD5"
        android:text="6"/>


    <Button
        android:id="@+id/KBD_Clear"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/KBD_Enter"
        android:layout_below="@+id/KBD_Cancel"
        android:text="BS"
        android:textSize="15sp"/>

    <Button
        android:id="@+id/KBD7"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="267dp"
        android:text="7"/>

    <Button
