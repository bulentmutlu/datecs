        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/KBD7"
        android:layout_marginStart="13dp"
        android:layout_toEndOf="@+id/KBD7"
        android:text="8"/>

    <Button
        android:id="@+id/KBD9"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/KBD8"
        android:layout_marginStart="13dp"
        android:layout_toEndOf="@+id/KBD8"
        android:text="9"/>

    <Button
        android:id="@+id/KBD_Enter"
        android:layout_width="60dp"
        android:layout_height="140dp"
        android:layout_alignBottom="@+id/KBD0"
        android:layout_alignTop="@+id/KBD0"
        android:layout_marginStart="13dp"
        android:layout_toEndOf="@+id/KBD9"
        android:text="OK"
        android:textColorLink="#ff40a6"/>


    <Button
        android:id="@+id/KBD0"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/KBD8"
        android:layout_below="@+id/KBD8"
        android:layout_marginTop="13dp"
        android:text="0"/>

    <TextView
        android:id="@+id/textViewPinDigitEx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/KBD1"
        android:layout_above="@+id/KBD2"
        android:layout_marginTop="80dp"
        android:textColor="@android:color/black"
        android:background="#ffffe0"
        android:textSize="20sp"/>
